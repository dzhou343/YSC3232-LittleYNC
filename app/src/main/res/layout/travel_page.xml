<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TravelActivity">

    <ImageView
        android:id="@+id/YNC_map_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:contentDescription="YNC_map_1"
        android:scaleType="centerCrop"
        android:src="@drawable/travel_page"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button2"
        android:layout_width="113dp"
        android:layout_height="99dp"
        android:layout_marginStart="257dp"
        android:layout_marginTop="160dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="344dp"
        android:onClick="armoryButton"
        android:text="Armory"
        android:alpha="0"
        app:layout_constraintBottom_toBottomOf="@+id/YNC_map_1"
        app:layout_constraintEnd_toEndOf="@+id/YNC_map_1"
        app:layout_constraintStart_toStartOf="@+id/YNC_map_1"
        app:layout_constraintTop_toTopOf="@+id/YNC_map_1" />

    <Button
        android:id="@+id/buttery_trading_hub"
        android:layout_width="138dp"
        android:layout_height="84dp"
        android:layout_marginStart="241dp"
        android:layout_marginTop="354dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="165dp"
        android:onClick="goToMarketplace"
        android:text="@string/marketplace_name"
        android:alpha="0"
        app:layout_constraintBottom_toBottomOf="@+id/YNC_map_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/YNC_map_1"
        app:layout_constraintTop_toTopOf="@+id/YNC_map_1" />

    <Button
        android:id="@+id/button4"
        android:layout_width="158dp"
        android:layout_height="90dp"
        android:layout_marginTop="16dp"
        android:onClick="cendanaForestButton"
        android:text="@string/woodcutting_name"
        android:alpha="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.532"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button5"
        android:layout_width="160dp"
        android:layout_height="104dp"
        android:layout_marginStart="47dp"
        android:layout_marginTop="460dp"
        android:layout_marginEnd="204dp"
        android:layout_marginBottom="39dp"
        android:onClick="battleButton"
        android:text="@string/battleground_name"
        android:alpha="0"
        app:layout_constraintBottom_toBottomOf="@+id/YNC_map_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/YNC_map_1"
        app:layout_constraintTop_toTopOf="@+id/YNC_map_1" />

    <Button
        android:id="@+id/button6"
        android:layout_width="146dp"
        android:layout_height="108dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="175dp"
        android:layout_marginEnd="249dp"
        android:layout_marginBottom="320dp"
        android:alpha="0"
        android:onClick="fishButton"
        android:text="@string/fishing_name"
        app:layout_constraintBottom_toBottomOf="@+id/YNC_map_1"
        app:layout_constraintEnd_toEndOf="@+id/YNC_map_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/YNC_map_1" />
</androidx.constraintlayout.widget.ConstraintLayout>